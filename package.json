	"devDependencies": {
		"concurrently": "4.1.2",
		"electron": "6.0.11",
		"electron-builder": "^21.2.0",
		"genversion": "^2.2.0",
		"wait-on": "3.3.0"
	},
	"main": "public/electron.js",
	"homepage": "./",
	"scripts": {
		"web-start": "./node_modules/.bin/genversion src/utils/AppVersion.js --es6 && node web-scripts/start.js",
		"web-build": "./node_modules/.bin/genversion src/utils/AppVersion.js --es6 && node web-scripts/build.js",
		"web-test": "./node_modules/.bin/genversion src/utils/AppVersion.js --es6 && node scripts/test.js",
		"electron-start": "concurrently \"cross-env BROWSER=none npm run web-start\" \"wait-on http://localhost:3000 && electron .\" ",
		"electron-build": "npm run web-build && electron-builder --mac --windows --linux",
		"electron-test": ""
	},
	"build": {
		"appId": "com.arcaneiceman.kraken.kraken-client",
		"files": [
			"*.js",
			"build",
			"public/electron.js"
		],
		"mac": {
			"target": [
				"dmg"
			],
			"icon": "src/assets/icons/application/icon.png",
			"category": "public.app-category.utilities"
		},
		"dmg": {
			"contents": [
				{
					"x": 110,
					"y": 150
				},
				{
					"x": 240,
					"y": 150,
					"type": "link",
					"path": "/Applications"
				}
			]
		},
		"linux": {
			"target": [
				"AppImage"
			],
			"category": "Security"
		},
		"win": {
			"target": [
				"portable"
			],
			"icon": "src/assets/icons/application/icon.png"
		}
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"jest": {
		"roots": [
			"<rootDir>/src"
		],
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}",
			"!src/**/*.d.ts"
		],
		"setupFiles": [
			"react-app-polyfill/jsdom"
		],
		"setupFilesAfterEnv": [],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
			"<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
		],
		"testEnvironment": "jest-environment-jsdom-fourteen",
		"transform": {
			"^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
			"^.+\\.module\\.(css|sass|scss)$"
		],
		"modulePaths": [],
		"moduleNameMapper": {
			"^react-native$": "react-native-web",
			"^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
		},
		"moduleFileExtensions": [
			"web.js",
			"js",
			"web.ts",
			"ts",
			"web.tsx",
			"tsx",
			"json",
			"web.jsx",
			"jsx",
			"node"
		],
		"watchPlugins": [
			"jest-watch-typeahead/filename",
			"jest-watch-typeahead/testname"
		]
	},
	"babel": {
		"presets": [
			"react-app"
		]
	}
}
